<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="30.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="gameOverText" strokeType="OUTSIDE" strokeWidth="0.0">
         <font>
            <Font size="34.0" />
         </font>
      </Text>
      <Text fx:id="subText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" visible="false" />
      <Button fx:id="restartButton" mnemonicParsing="false" onMouseClicked="#restartLevel" text="Restart This Level" />
      <Button fx:id="mainMenuButton" mnemonicParsing="false" onMouseClicked="#mainMenu" text="Main Menu" />
      <Button mnemonicParsing="false" onMouseClicked="#quitGame" text="Quit" />
   </children>
</VBox>
